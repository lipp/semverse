{
    "name": "semverse",
    "version": "1.0.0",
    "description": "A small universe, SemVer based",
    "main": "index.js",
    "config": {},
    "scripts": {
        "lint": "jslint 'src/**/*.js' 'tests/**/*.js'",
        "generate-tap": "tape 'src/**/*.spec.js' > tap.log",
        "pretty-tap": "cat tap.log | tap-spec",
        "raw-tap": "cat tap.log | faucet",
        "unit-tests": "npm run generate-tap",
        "local-dev-raw": "npm run generate-tap && npm run raw-tap",
        "local-dev-pretty": "npm run generate-tap && npm run pretty-tap",
        "test": "NODE_ENV=test nyc --reporter=html --all npm run unit-tests",
        "test-ci": "NOVE_ENV=test nyc --reporter=text-lcov --all --check-coverage --lines 100 npm run unit-tests | coveralls",
        "start": "nodemon src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StephaneTrebel/semverse.git"
    },
    "keywords": [
        "semverse",
        "semver",
        "rest",
        "api",
        "swagger",
        "sequelize"
    ],
    "author": "Stéphane Trébel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/StephaneTrebel/semverse/issues"
    },
    "homepage": "https://github.com/StephaneTrebel/semverse#readme",
    "devDependencies": {
        "coveralls": "^2.11.9",
        "faucet": "0.0.1",
        "js-beautify": "^1.6.2",
        "nyc": "^6.4.0",
        "proxyquire": "^1.7.9",
        "sinon": "^1.17.3",
        "tap-spec": "^4.1.1",
        "tape": "^4.5.1"
    },
    "dependencies": {
        "express": "^4.13.4",
        "lodash": "^4.7.0",
        "sequelize": "^3.20.0",
        "swagger-router": "^0.4.1",
        "swagger-tools": "^0.10.1",
        "swagger-ui": "^2.1.4"
    },
    "nyc": {
        "exclude": [
            "coverage",
            "src/index.spec.js",
            "'**/*.spec.js'",
            "tests"
        ]
    }
}
