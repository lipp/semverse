{
    "name": "semverse",
    "version": "6.0.0",
    "engines": {
        "node": ">=6.0.0",
        "npm": ">=3.8.6"
    },
    "description": "A small universe, SemVer based",
    "main": "index.js",
    "config": {},
    "scripts": {
        "validate-spec": "swagger-tools validate src/api/swagger.json -v",
        "docs": "jsdoc -c jsdoc.json",
        "lint": "jslint 'src/**/*.js'",
        "tape-console": "NODE_ENV=test tape 'src/**/*.spec.js'",
        "tape-faucet": "npm run tape-console | faucet",
        "test": "npm run validate-spec && npm run lint && nyc --reporter=text --all npm run tape-faucet && nyc report --reporter=html && nyc check-coverage --lines 100 --functions 100 --branches 100",
        "test-ci": "npm run validate-spec && npm run lint && nyc --reporter=text-lcov --all --check-coverage --lines 100 npm run tape-spec | coveralls",
        "start": "NODE_ENV=production nodemon src/index.js",
        "new-git-tag": "git tag -a $npm_package_version -m $npm_package_version",
        "commit-new-version": "git add --all && git commit -m $npm_package_version -m '(Autogenerated, please update !)'",
        "bump-version": "echo Have you updated the version number ? If not please Ctrl-C NOW ! && read test",
        "new-version": "npm run bump-version && npm run test && npm run docs && npm run new-git-tag && npm run commit-new-version"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StephaneTrebel/semverse.git"
    },
    "keywords": [
        "semverse",
        "semver",
        "rest",
        "api",
        "swagger",
        "sequelize"
    ],
    "author": "Stéphane Trébel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/StephaneTrebel/semverse/issues"
    },
    "homepage": "https://github.com/StephaneTrebel/semverse#readme",
    "devDependencies": {
        "blue-tape": "^0.2.0",
        "coveralls": "^2.11.9",
        "faucet": "0.0.1",
        "js-beautify": "^1.6.2",
        "jsdoc": "^3.4.0",
        "jslint": "^0.10.1",
        "nyc": "^6.4.0",
        "proxyquire": "^1.7.9",
        "sinon": "^1.17.3",
        "tap-spec": "^4.1.1",
        "tape": "^4.5.1"
    },
    "dependencies": {
        "express": "^4.13.4",
        "lodash": "^4.7.0",
        "nodemon": "^1.9.2",
        "sequelize": "^3.20.0",
        "swagger-router": "^0.4.1",
        "swagger-tools": "^0.10.1",
        "swagger-ui": "^2.1.4"
    },
    "nyc": {
        "exclude": [
            "coverage",
            "docs",
            "'**/*.spec.js'",
            "tests",
            "src/models"
        ]
    }
}
